<section id="venue-edit">
	<div class="container">
		<div class="row">
			<div class="col-sm-12">
				<%= render :partial => 'form' %>
				<%= link_to @venue, title: 'Show', class: 'btn btn-primary' do %><i class="fa fa-undo left"></i>Show<% end %>&nbsp;<%= link_to venue_path(@venue), title: 'Delete', method: :delete, data: { confirm: "Are you sure you would like to delete #{@venue.name}?" }, class: 'btn btn-danger' do %><i class="fa fa-trash-o left"></i>Delete<% end %>
			</div>
		</div>
	</div>
</section>