<section id="users-all" class="pad">
	<div class="container">
		<div class="row">
			<div class="col-sm-12">
				<ul>
					<% @users.each do |user| %>
			    <li><%= link_to user, title: user.fullname do %><%= user.fullname %><% end %></li>
			    <% end %>
				</ul>
			</div>
		</div>
	</div>
</section>